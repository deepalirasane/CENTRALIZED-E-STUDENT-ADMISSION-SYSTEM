                                <p class="help-block text-danger"></p>
                            </div>
                        </div>
                        
                                          
                             <div class="row control-group">
                            <div class="form-group col-xs-12 floating-label-form-group controls">
                                <label>Message</label>
                                <textarea rows="2" class="form-control" maxlength="150" name="message" style="font-size:18px;" placeholder="Message" id="message" required data-validation-required-message="Please enter a message."></textarea>
                                <p class="help-block text-danger"></p>
                            </div>
                        </div>
                        <br>
                        <div id="success"></div>
                        <div class="row">
                            <div class="form-group col-xs-12">
                              <Center> <button type="submit"  id="contactbtn "class="btn btn-primary btn-medium">Send</button> </Center>
                            </div>
                        </div>
                   }
                </div>
            </div>
        </div>
    </section>
